import math
3
side=int(input())
4
length=int(input())
5
breadth=int(input())
6
radius=int(input())
7
base=int(input())
8
height=int(input())
9
square=side*side
10
rectangle=length*breadth
11
circle=math.pi*radius*radius
12
triangle=0.5*base*height
13
print("Area of Square="+str(square))
14
print("Area of Rectangle="+str(rectangle))
15
print(f"Area of Circle={int(circle*100)/100}")
16
print("Area of Triangle="+str(int(triangle)))
